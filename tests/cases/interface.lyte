
interface Compare<A> {
    cmp(lhs: typevar A, rhs: typevar A) -> i32
}

sort<T>(array: [typevar T]) -> [typevar T] where Compare<T> {
    // We aren't actually doing any sorting, just need
    // to verify that calling the function type checks.
    cmp(array[0], array[1])
    array
}

test {
    sort([1,2,3])
}
